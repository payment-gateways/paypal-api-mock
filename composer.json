{
    "name": "payment-gateways/paypal-api-mock",
    "description": "Mock for PayPal Apis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dar√≠o Rivera",
            "email": "fermius.us@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PaymentGateway\\PayPalApiMock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaymentGateway\\PayPalApiMock\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit --stop-on-error --stop-on-failure --testdox",
        "phpunit-xml": "phpunit --stop-on-error --stop-on-failure --coverage-clover phpunit.coverage.xml"
    }
}
